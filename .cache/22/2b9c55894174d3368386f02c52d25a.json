\r\n\r\n  onMovieClick(movie) {\r\n    this.setState({\r\n      selectedMovie: movie\r\n    });\r\n  }\r\n\r\n  onMainViewClick() {\r\n    this.setState({\r\n      selectedMovie: null\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const {movies, selectedMovie } = this.state;\r\n\r\n    // before movies have been loaded\r\n    if (!movies) return <div className = \"main-view\"/>;\r\n\r\n    return (\r\n      <div className = \"main-view\">\r\n      {/* if there is a selectedMovie, assign it to the MovieView movie,\r\n       else onClick assign the selectedMovie to movie */}\r\n      {selectedMovie\r\n        ? <MovieView\r\n            movie={selectedMovie}\r\n            onClick = {button => this.onMainViewClick()}/>\r\n        : movies.map(movie => (\r\n          <MovieCard\r\n            key={movie._id}\r\n            movie={movie}\r\n            onClick={movie => this.onMovieClick(movie)}/>\r\n        ))\r\n      }\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"1797c52fd9d57c720345c430e907e37f","cacheData":{"env":{}}}